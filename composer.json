{
    "name": "nacosvel/load-balancer",
    "description": "Implementing Several Load Balancing Scheduling Algorithms with PHP",
    "keywords": [
        "load balancer",
        "best available rule",
        "random rule",
        "round rule",
        "weighted response time rule",
        "weighted random rule",
        "weighted round rule",
        "zone limitation rule"
    ],
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "nacosvel/helper": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nacosvel\\LoadBalancer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nacosvel\\LoadBalancer\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
